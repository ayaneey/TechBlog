<div class="comments">
	{{#each this}}
		<div class="card">
			<h3 class="card-header"></h3>
			<section class="comment-body">
				<div class="text">
					{{comment_text}}
				</div>
				<div class="comment-by">
					-{{user.username}},
					{{format_date created_at}}
				</div>
			</section>
		</div>
	{{/each}}
</div>